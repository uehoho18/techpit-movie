<%= form_with(model: @post, local: true) do |form| %>
  <div class="form-group">
    <%= form.text_field :title, placeholder: "タイトル" %>
  </div>
  <div class="form-group">
    <%= form.text_area :body, placeholder: "動画の説明" %>
  </div>
  <div class="form-group">
    <%= form.file_field :video %>
  </div>
  <%= form.submit class: "btn btn-primary"%>
<% end %>
